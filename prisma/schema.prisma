generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model AvgWinnerPrizes {
  ID_EVENT_INVENTORY_ITEM Int     @id
  ID_EVENT                Int?
  PRIZE_VALUE             BigInt?
  event                   Events? @relation("AVGEVENTPRIZES", fields: [ID_EVENT], references: [ID_EVENT])
}

model Basket {
  ID_BASKET      Int   @id
  ID_RACE        Int?
  BASKET_NO      Int?
  CAPACITY       Int?
  IS_RACE_BASKET Int?
  race           Race? @relation(fields: [ID_RACE], references: [ID_RACE])
}

model BettingScheme {
  ID_BETTING_SCHEME       Int                      @id
  BETTING_SCHEME_NAME     String?
  BETTING_CUT_PERCENT     Float?
  BELGIAN_SHOW_1          Float?
  BELGIAN_SHOW_2          Float?
  BELGIAN_SHOW_3          Float?
  BELGIAN_SHOW_4          Float?
  BELGIAN_SHOW_5          Float?
  BELGIAN_SHOW_6          Float?
  BELGIAN_SHOW_7          Float?
  STANDARD_SHOW_1         Float?
  STANDARD_SHOW_2         Float?
  STANDARD_SHOW_3         Float?
  STANDARD_SHOW_4         Float?
  STANDARD_SHOW_5         Float?
  STANDARD_SHOW_6         Float?
  WTA_1                   Float?
  WTA_2                   Float?
  WTA_3                   Float?
  WTA_4                   Float?
  WTA_5                   Float?
  BETTINGSCHEMEEVENTS     Events[]                 @relation("BETTINGSCHEMEEVENTS")
  standardShowPercentages StandardShowPercentage[]
}

model Birds {
  ID_BIRD              Int                  @id
  BAND                 String?
  BAND_1               String?
  BAND_2               String?
  BAND_3               String?
  BAND_4               String?
  BIRD_NAME            String?
  RF_ID                String?
  COLOR                String?
  SEX                  Int?
  IS_ACTIVE            Int?
  IS_RACE_VERIFIED     Int?
  IS_LOST              Int?
  LOST_DATE            DateTime?
  LOST_ID_RACE         Int?
  NOTE                 String?
  ID_PICTURE           Int?
  PLAY_ATTENTION_SOUND Int?
  breederId            Int?
  breeder              Breeders?            @relation(fields: [breederId], references: [ID_BREEDER])
  lostRace             Race?                @relation("LOSTRACE", fields: [LOST_ID_RACE], references: [ID_RACE])
  picture              Picture?             @relation("BIRDPICTURE", fields: [ID_PICTURE], references: [ID_PICTURE])
  eventInventoryItems  EventInventoryItem[]
  lostHistories        LostHistory[]
  racePhantomBirds     RacePhantomBird[]
}

model BirdsView {
  ID_BIRD              Int       @id
  BAND                 String?
  BAND_1               String?
  BAND_2               String?
  BAND_3               String?
  BAND_4               String?
  BIRD_NAME            String?
  RF_ID                String?
  COLOR                String?
  SEX                  Int?
  IS_ACTIVE            Int?
  IS_RACE_VERIFIED     Int?
  IS_LOST              Int?
  LOST_DATE            DateTime?
  LOST_ID_RACE         Int?
  NOTE                 String?
  ID_PICTURE           Int?
  PLAY_ATTENTION_SOUND Int?
  lostRace             Race?     @relation("VIEWLOSTRACE", fields: [LOST_ID_RACE], references: [ID_RACE])
  picture              Picture?  @relation("VIEWBIRDPICTURE", fields: [ID_PICTURE], references: [ID_PICTURE])
}

model Breeders {
  ID_BREEDER             Int              @id
  NUMBER                 Int?
  FIRST_NAME             String?
  LAST_NAME              String?
  COUNTRY                String?
  IS_DEFAULT_ADDRESS_1   Int?
  ADDRESS_1              String?
  CITY_1                 String?
  STATE_1                String?
  ZIP_1                  String?
  ADDRESS_2              String?
  CITY_2                 String?
  STATE_2                String?
  ZIP_2                  String?
  PHONE                  String?
  CELL                   String?
  FAX                    String?
  EMAIL                  String?
  EMAIL_2                String?
  WEB_ADDRESS            String?
  SOCIAL_SECURITY_NUMBER String?
  STATUS                 Int?
  STATUS_DATE            DateTime?
  NOTE                   String?
  LOGIN_NAME             String?
  LOGIN_PASSWORD         String?
  ID_PICTURE             Int?
  SMS                    String?
  TAX_NUMBER             String?
  DEF_NAME_AGN           String?
  DEF_NAME_AS            String?
  picture                Picture?         @relation(fields: [ID_PICTURE], references: [ID_PICTURE])
  eventInventories       EventInventory[]
  partners               Partners[]
  birds                  Birds[]
}

model DatasyncSys {
  ID        Int    @id
  DSEXT_VER Float?
}

model DatasyncWeb {
  ID               Int     @id
  PROCESSED        Int?
  DBTABLE          String?
  DBOPERATION      String?
  DBTABLEIDCOLNAME String?
  DBTABLEROWID     Int?
  DBTABLEROW       Bytes?
}

model Events {
  ID_EVENT                     Int               @id
  EVENT_NAME                   String?
  EVENT_SHORT_NAME             String?
  EVENT_DATE                   DateTime?
  ID_FEE_SCHEME                Int?
  FEE_SCHEME                   FeeScheme?        @relation(fields: [ID_FEE_SCHEME], references: [ID_FEE_SCHEME])
  ID_FINAL_PRIZE_SCHEME        Int?
  FINAL_PRIZE_SCHEME           PrizeScheme?      @relation(fields: [ID_FINAL_PRIZE_SCHEME], references: [ID_PRIZE_SCHEME], name: "FINALPRIZEEVENTS")
  ID_HOT_SPOT1_PRIZE_SCHEME    Int?
  HOT_SPOT1_PRIZE_SCHEME       PrizeScheme?      @relation(fields: [ID_HOT_SPOT1_PRIZE_SCHEME], references: [ID_PRIZE_SCHEME], name: "HOTSPOT1PRIZEEVENTS")
  ID_HOT_SPOT2_PRIZE_SCHEME    Int?
  HOT_SPOT2_PRIZE_SCHEME       PrizeScheme?      @relation(fields: [ID_HOT_SPOT2_PRIZE_SCHEME], references: [ID_PRIZE_SCHEME], name: "HOTSPOT2PRIZEEVENTS")
  ID_HOT_SPOT3_PRIZE_SCHEME    Int?
  HOT_SPOT3_PRIZE_SCHEME       PrizeScheme?      @relation(fields: [ID_HOT_SPOT3_PRIZE_SCHEME], references: [ID_PRIZE_SCHEME], name: "HOTSPOT3PRIZEEVENTS")
  ID_HOT_SPOT_AVG_PRIZE_SCHEME Int?
  HOT_SPOT_AVG_PRIZE_SCHEME    PrizeScheme?      @relation(fields: [ID_HOT_SPOT_AVG_PRIZE_SCHEME], references: [ID_PRIZE_SCHEME], name: "HOTSPOTAVGPRIZEEVENTS")
  ID_BETTING_SCHEME            Int?
  BETTING_SCHEME               BettingScheme?    @relation(fields: [ID_BETTING_SCHEME], references: [ID_BETTING_SCHEME], name: "BETTINGSCHEMEEVENTS")
  IS_OPEN                      Int?
  EVENT_TYPE                   Int?
  avgWinnerPrizes              AvgWinnerPrizes[] @relation("AVGEVENTPRIZES")
  eventInventories             EventInventory[]
  eventRaceNumbers             EventRaceNumber[]
  prizeValues                  PrizeValue[]
  races                        Race[]
  racePhantomBirds             RacePhantomBird[]
}

model EventInventory {
  ID_EVENT_INVENTORY  Int                  @id
  ID_EVENT            Int?
  ID_BREEDER          Int?
  SIGN_IN_DATE        DateTime?
  WAITING_DATE        DateTime?
  IS_WAITING          Int?
  RESERVED_BIRDS      Int?
  LOFT                String?
  NOTE                String?
  event               Events?              @relation(fields: [ID_EVENT], references: [ID_EVENT])
  breeder             Breeders?            @relation(fields: [ID_BREEDER], references: [ID_BREEDER])
  eventInventoryItems EventInventoryItem[]
  payments            Payments[]
  partners            Partners[]
}

model EventInventoryItem {
  ID_EVENT_INVENTORY_ITEM Int                  @id
  ID_EVENT_INVENTORY      Int?
  ID_BIRD                 Int?
  BIRD_NO                 Int?
  ARRIVAL_DATE            DateTime?
  DEPARTURE_DATE          DateTime?
  PERCH_FEE_VALUE         Float?
  ENTRY_FEE_VALUE         Float?
  ENTRY_FEE_PAID          Int?
  ENTRY_REFUND            Float?
  BETS_REFUND             Float?
  HOT_SPOT_FEE_VALUE      Float?
  HOT_SPOT_REFUND         Float?
  ID_REPLACED_ITEM        Int?
  IS_BACKUP               Int?
  BELGIAN_SHOW_BET_1      Float?
  BELGIAN_SHOW_BET_2      Float?
  BELGIAN_SHOW_BET_3      Float?
  BELGIAN_SHOW_BET_4      Float?
  BELGIAN_SHOW_BET_5      Float?
  BELGIAN_SHOW_BET_6      Float?
  BELGIAN_SHOW_BET_7      Float?
  STANDARD_SHOW_BET_1     Float?
  STANDARD_SHOW_BET_2     Float?
  STANDARD_SHOW_BET_3     Float?
  STANDARD_SHOW_BET_4     Float?
  STANDARD_SHOW_BET_5     Float?
  STANDARD_SHOW_BET_6     Float?
  WTA_BET_1               Float?
  WTA_BET_2               Float?
  WTA_BET_3               Float?
  WTA_BET_4               Float?
  WTA_BET_5               Float?
  IS_BET_ACTIVE           Float?
  TRANSFER_DUE            BigInt?
  eventInventory          EventInventory?      @relation(fields: [ID_EVENT_INVENTORY], references: [ID_EVENT_INVENTORY])
  bird                    Birds?               @relation(fields: [ID_BIRD], references: [ID_BIRD])
  replacedItem            EventInventoryItem?  @relation("REPLACEDITEM", fields: [ID_REPLACED_ITEM], references: [ID_EVENT_INVENTORY_ITEM])
  replacementItems        EventInventoryItem[] @relation("REPLACEDITEM")
  raceIgnoreBirds         RaceIgnoreBird[]
  raceItems               RaceItem[]
  tmpBets                 TmpBets[]
}

model EventRaceNumber {
  ID_NUMBER_GROUP   Int
  ID_EVENT          Int
  NUMBER_RANGE_FROM Int?
  NUMBER_RANGE_TO   Int?
  numberGroup       RaceNumberGroup? @relation(fields: [ID_NUMBER_GROUP], references: [ID_RACE_NUMBER_GROUP])
  event             Events?          @relation(fields: [ID_EVENT], references: [ID_EVENT])

  @@unique([ID_NUMBER_GROUP, ID_EVENT])
}

model FeeScheme {
  ID_FEE_SCHEME         Int            @id
  FEE_SCHEME_NAME       String?
  ENTRY_FEE             BigInt?
  IS_REFUNDABLE         Int?
  MIN_ENTRY_FEES        Int?
  MAX_BIRD_COUNT        Int?
  MAX_BACKUP_BIRD_COUNT Int?
  IS_FLOATING_BACKUP    Int?
  FEES_CUT_PERCENT      BigInt?
  HOT_SPOT1_FEE         BigInt?
  HOT_SPOT2_FEE         BigInt?
  HOT_SPOT3_FEE         BigInt?
  HOT_SPOT_FINAL_FEE    BigInt?
  events                Events[]
  perchFeeItems         PerchFeeItem[]
}

model IbeLogBlobFields {
  LOG_TABLES_ID  BigInt  @id
  FIELD_NAME     String?
  OLD_CHAR_VALUE String?
  NEW_CHAR_VALUE String?
  OLD_BLOB_VALUE Bytes?
  NEW_BLOB_VALUE Bytes?
}

model IbeLogFields {
  LOG_TABLES_ID BigInt  @id
  FIELD_NAME    String?
  OLD_VALUE     String?
  NEW_VALUE     String?
}

model IbeLogKeys {
  LOG_TABLES_ID BigInt  @id
  KEY_FIELD     String?
  KEY_VALUE     String?
}

model IbeLogTables {
  ID         BigInt    @id
  TABLE_NAME String?
  OPERATION  String?
  DATE_TIME  DateTime?
  USER_NAME  String?
}

model Log {
  ID_LOG         Int                @id
  LOG_LEVEL      String?
  CATEGORY       String?
  CONTENT        String?
  DT_LOGGED      DateTime?
  ID_USER        Int?
  MACHINE        String?
  user           Users?             @relation(fields: [ID_USER], references: [ID_USER])
  userLoggedins  LogUserLoggedin[]
  userLoggedouts LogUserLoggedout[]
}

model LogUserLoggedin {
  ID_LOG  Int
  ID_USER Int
  log     Log   @relation(fields: [ID_LOG], references: [ID_LOG])
  user    Users @relation(fields: [ID_USER], references: [ID_USER])

  @@unique([ID_LOG, ID_USER])
}

model LogUserLoggedout {
  ID_LOG       Int
  ID_LOGGED_IN Int
  log          Log @relation(fields: [ID_LOG], references: [ID_LOG])

  @@unique([ID_LOG, ID_LOGGED_IN])
}

model LostHistory {
  ID_LOST_HISTORY Int       @id
  ID_BIRD         Int?
  LOST_DATE       DateTime?
  IS_LOST         Int?
  ID_RACE         Int?
  bird            Birds?    @relation(fields: [ID_BIRD], references: [ID_BIRD])
  race            Race?     @relation(fields: [ID_RACE], references: [ID_RACE])
}

model OrganizerData {
  NAME    String?
  ADDRESS String?
  CITY    String?
  STATE   String?
  ZIP     String?
  EMAIL   String  @id
  WEB     String?
  PHONE   String?
  CELL    String?
  FAX     String?
  NOTE    String?
}

model Partners {
  ID_EVENT_INVENTORY Int
  ID_BREEDER         Int
  eventInventory     EventInventory @relation(fields: [ID_EVENT_INVENTORY], references: [ID_EVENT_INVENTORY])
  breeder            Breeders       @relation(fields: [ID_BREEDER], references: [ID_BREEDER])

  @@unique([ID_EVENT_INVENTORY, ID_BREEDER])
}

model Payments {
  ID_PAYMENT         Int             @id
  ID_EVENT_INVENTORY Int?
  PAYMENT_TYPE       Int?
  PAYMENT_DATE       DateTime?
  PAYMENT_VALUE      Float?
  PAYMENT_METHOD     Int?
  PAYMENT_DESC       String?
  PAYMENT_TIMESTAMP  DateTime?
  ID_PICTURE         Int?
  eventInventory     EventInventory? @relation(fields: [ID_EVENT_INVENTORY], references: [ID_EVENT_INVENTORY])
  picture            Picture?        @relation(fields: [ID_PICTURE], references: [ID_PICTURE])
}

model PerchFeeItem {
  ID_PERCH_FEE_ITEM Int        @id
  BIRD_NO           Int?
  PERCH_FEE         BigInt?
  ID_FEE_SCHEME     Int?
  feeScheme         FeeScheme? @relation(fields: [ID_FEE_SCHEME], references: [ID_FEE_SCHEME])
}

model Picture {
  ID_PICTURE     Int         @id
  PICTURE        Bytes?
  PICTURE_TYPE   String?
  PICTURE_LAYOUT String?
  birds          Birds[]     @relation("BIRDPICTURE")
  birdsView      BirdsView[] @relation("VIEWBIRDPICTURE")
  breeders       Breeders[]
  payments       Payments[]
}

model PrizeScheme {
  ID_PRIZE_SCHEME       Int               @id
  PRIZE_NAME            String?
  HOTSPOT1PRIZEEVENTS   Events[]          @relation("HOTSPOT1PRIZEEVENTS")
  HOTSPOT2PRIZEEVENTS   Events[]          @relation("HOTSPOT2PRIZEEVENTS")
  HOTSPOT3PRIZEEVENTS   Events[]          @relation("HOTSPOT3PRIZEEVENTS")
  HOTSPOTAVGPRIZEEVENTS Events[]          @relation("HOTSPOTAVGPRIZEEVENTS")
  FINALPRIZEEVENTS      Events[]          @relation("FINALPRIZEEVENTS")
  prizeSchemeItems      PrizeSchemeItem[]
}

model PrizeSchemeItem {
  ID_PRIZE_SCHEME_ITEM Int          @id
  FROM_POSITION        Int?
  TO_POSITION          Int?
  PRIZE_VALUE          Float?
  ID_PRIZE_SCHEME      Int?
  prizeScheme          PrizeScheme? @relation(fields: [ID_PRIZE_SCHEME], references: [ID_PRIZE_SCHEME])
  prizeValues          PrizeValue[]
}

model PrizeValue {
  ID_PRIZE_VALUE       Int              @id
  ID_EVENT             Int?
  ID_RACE_TYPE         Int?
  ID_PRIZE_SCHEME_ITEM Int?
  PRIZE_VALUE          Float?
  event                Events?          @relation(fields: [ID_EVENT], references: [ID_EVENT])
  raceType             RaceType?        @relation(fields: [ID_RACE_TYPE], references: [ID_RACE_TYPE])
  prizeSchemeItem      PrizeSchemeItem? @relation(fields: [ID_PRIZE_SCHEME_ITEM], references: [ID_PRIZE_SCHEME_ITEM])
}

model Race {
  ID_RACE             Int               @id
  ID_RACE_TYPE        Int?
  RACE_NUMBER         Int?
  ID_EVENT            Int?
  LOCATION            String?
  DESCRIPTION         String?
  DISTANCE            Int?
  START_TIME          DateTime?
  END_TIME            DateTime?
  SUNRISE             DateTime?
  SUNSET              DateTime?
  IS_CLOSED           Int?
  WEATHER             String?
  WIND                String?
  TEMPERATURE         String?
  ARRIVAL_WEATHER     String?
  ARRIVAL_WIND        String?
  ARRIVAL_TEMPERATURE String?
  raceType            RaceType?         @relation(fields: [ID_RACE_TYPE], references: [ID_RACE_TYPE])
  event               Events?           @relation(fields: [ID_EVENT], references: [ID_EVENT])
  baskets             Basket[]
  lostBirds           Birds[]           @relation("LOSTRACE")
  viewLostBirds       BirdsView[]       @relation("VIEWLOSTRACE")
  lostHistories       LostHistory[]
  raceIgnoreBirds     RaceIgnoreBird[]
  raceItems           RaceItem[]
  raceItemScans       RaceItemScan[]
  racePhantomBirds    RacePhantomBird[]
}

model RaceIgnoreBird {
  ID_IGNORE_BIRD          Int                 @id
  ID_RACE                 Int?
  ID_EVENT_INVENTORY_ITEM Int?
  NOTE                    String?
  race                    Race?               @relation(fields: [ID_RACE], references: [ID_RACE])
  eventInventoryItem      EventInventoryItem? @relation(fields: [ID_EVENT_INVENTORY_ITEM], references: [ID_EVENT_INVENTORY_ITEM])
}

model RaceItem {
  ID_RACE_ITEM      Int                 @id
  ID_RACE           Int?
  ID_INVENTORY_ITEM Int?
  ID_DIST_BASKET    Int?
  ID_RACE_BASKET    Int?
  IS_DIST_BASKETED  Int?
  IS_LOST           Int?
  LOST_ID_RACE      Int?
  RACE_BASKET_TIME  DateTime?
  race              Race?               @relation(fields: [ID_RACE], references: [ID_RACE])
  inventoryItem     EventInventoryItem? @relation(fields: [ID_INVENTORY_ITEM], references: [ID_EVENT_INVENTORY_ITEM])
  raceItemResult    RaceItemResult?
  raceItemScans     RaceItemScan[]
}

model RaceItemResult {
  ID_RACE_ITEM           Int       @id
  BIRD_POSITION          Int?
  BIRD_POSITION_HOT_SPOT Int?
  ARRIVAL_TIME           DateTime?
  BIRD_DROP              Int?
  PRIZE_VALUE            Float?
  raceItem               RaceItem  @relation(fields: [ID_RACE_ITEM], references: [ID_RACE_ITEM])
}

model RaceItemScan {
  ID_RACE      Int
  ID_RACE_ITEM Int
  SCAN_POS     Int?
  race         Race     @relation(fields: [ID_RACE], references: [ID_RACE])
  raceItem     RaceItem @relation(fields: [ID_RACE_ITEM], references: [ID_RACE_ITEM])

  @@unique([ID_RACE, ID_RACE_ITEM])
}

model RaceNumberGroup {
  ID_RACE_NUMBER_GROUP Int               @id
  eventRaceNumbers     EventRaceNumber[]
  raceTypes            RaceType[]
}

model RacePhantomBird {
  ID_RACE_PHANTOM_BIRD Int       @id
  ID_RACE              Int?
  ID_BIRD              Int?
  RF_ID                String?
  ARRIVAL_TIME         DateTime?
  ID_EVENT             Int?
  race                 Race?     @relation(fields: [ID_RACE], references: [ID_RACE])
  bird                 Birds?    @relation(fields: [ID_BIRD], references: [ID_BIRD])
  event                Events?   @relation(fields: [ID_EVENT], references: [ID_EVENT])
}

model RaceType {
  ID_RACE_TYPE    Int              @id
  TYPE_NAME       String?
  ID_NUMBER_GROUP Int?
  numberGroup     RaceNumberGroup? @relation(fields: [ID_NUMBER_GROUP], references: [ID_RACE_NUMBER_GROUP])
  prizeValues     PrizeValue[]
  races           Race[]
}

model Reports {
  ID_REPORT          Int     @id
  IDENT              String?
  CLASS_             String?
  FASTREPORT_CONTENT Bytes?
  DEFAULT_CAPTION    String?
  IS_DELETED         Int?
}

model ReportClasses {
  IDENT   String  @id
  CAPTION String?
  COPIES  Int?
  TYPE    String?
}

model ReportIds {
  ID Int @id
}

model RightGroups {
  ID_RIGHT_GROUP  Int               @id
  NAME            String?
  rightGroupLines RightGroupLines[]
  userRightGroups UserRightGroups[]
}

model RightGroupLines {
  ID_RIGHT_GROUP_LINE Int          @id
  ID_RIGHT_GROUP      Int?
  RIGHT_CODE          String?
  GRANT_LEVEL         Int?
  rightGroup          RightGroups? @relation(fields: [ID_RIGHT_GROUP], references: [ID_RIGHT_GROUP])
}

model StandardShowPercentage {
  ID_STANDARD_SHOW_PERCENTAGE Int            @id
  ID_BETTING_SCHEME           Int?
  PLACE                       Int?
  PERC_VALUE                  BigInt?
  bettingScheme               BettingScheme? @relation(fields: [ID_BETTING_SCHEME], references: [ID_BETTING_SCHEME])
}

model SystemCfg {
  CFG_KEY   String  @id
  CFG_VALUE String?
}

model SystemInfo {
  INFO_KEY   String  @id
  INFO_VALUE String?
}

model SystemRequirements {
  ID_INFO_UPDATE   Int            @id
  MODULE_NAME      String?
  REQUIRED_VERSION String?
  ID_SYSTEM_UPDATE Int?
  systemUpdate     SystemUpdates? @relation(fields: [ID_SYSTEM_UPDATE], references: [ID_SYSTEM_UPDATE])
}

model SystemSession {
  SESSION_KEY   String  @id
  SESSION_VALUE String?
}

model SystemUpdates {
  ID_SYSTEM_UPDATE   Int                  @id
  PATCH_VERSION      String?
  DATETIME_EXECUTION DateTime?
  CHANGE_INFO        Bytes?
  systemRequirements SystemRequirements[]
}

model TmpAvgSpeed {
  ID_EVENT_INVENTORY_ITEM Int     @id
  BIRD_POSITION           Int?
  FLIGHT_TIME             BigInt?
  FLIGHT_DISTANCE         Int?
  AVG_SPEED_IN_YARDS      BigInt?
  AVG_SPEED_IN_MILES      BigInt?
}

model TmpBets {
  ID_RACE_ITEM            Int                 @id
  ID_EVENT_INVENTORY_ITEM Int?
  BIRD_DROP               Int?
  BELGIAN_SHOW_1          BigInt?
  BELGIAN_SHOW_2          BigInt?
  BELGIAN_SHOW_3          BigInt?
  BELGIAN_SHOW_4          BigInt?
  BELGIAN_SHOW_5          BigInt?
  BELGIAN_SHOW_6          BigInt?
  BELGIAN_SHOW_7          BigInt?
  STANDARD_SHOW_1         BigInt?
  STANDARD_SHOW_2         BigInt?
  STANDARD_SHOW_3         BigInt?
  STANDARD_SHOW_4         BigInt?
  STANDARD_SHOW_5         BigInt?
  STANDARD_SHOW_6         BigInt?
  WTA_1                   BigInt?
  WTA_2                   BigInt?
  WTA_3                   BigInt?
  WTA_4                   BigInt?
  WTA_5                   BigInt?
  TOTAL                   BigInt?
  EXEC_TIME               DateTime?
  eventInventoryItem      EventInventoryItem? @relation(fields: [ID_EVENT_INVENTORY_ITEM], references: [ID_EVENT_INVENTORY_ITEM])
}

model TmpId {
  ID  Int     @id
  TAG String?
}

model Users {
  ID_USER         Int               @id
  LOGIN_NAME      String?
  FULL_NAME       String?
  PASSWORD        String?
  IS_ACTIVE       Int?
  EMAIL_ADDRESS   String?
  logs            Log[]
  userLoggedins   LogUserLoggedin[]
  userRightGroups UserRightGroups[]
}

model UserRightGroups {
  ID_USER_RIGHT_GROUP Int          @id
  ID_USER             Int?
  ID_RIGHT_GROUP      Int?
  user                Users?       @relation(fields: [ID_USER], references: [ID_USER])
  rightGroup          RightGroups? @relation(fields: [ID_RIGHT_GROUP], references: [ID_RIGHT_GROUP])
}
