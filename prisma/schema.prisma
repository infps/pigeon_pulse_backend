// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

// Looking for ways to speed up your queries, or scale easily with your serverless or edge functions?
// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model AvgWinnerPrizes {
  idEventInventoryItem Int     @id @default(autoincrement()) @map("ID_EVENT_INVENTORY_ITEM")
  idEvent              Int?    @map("ID_EVENT")
  prizeValue           Int?    @map("PRIZE_VALUE")
  event                Events? @relation("AVGEVENTPRIZES", fields: [idEvent], references: [idEvent])

  @@map("AvgWinnerPrizes")
}

model Basket {
  idBasket        Int        @id @default(autoincrement()) @map("ID_BASKET")
  idRace          Int?       @map("ID_RACE")
  basketNo        Int?       @map("BASKET_NO")
  capacity        Int?       @map("CAPACITY")
  isRaceBasket    Int?       @map("IS_RACE_BASKET")
  race            Race?      @relation(fields: [idRace], references: [idRace])
  distBasketItems RaceItem[] @relation("DistBasketItems")
  raceBasketItems RaceItem[] @relation("RaceBasketItems")

  @@map("Basket")
}

model BettingScheme {
  idBettingScheme         Int                      @id @default(autoincrement()) @map("ID_BETTING_SCHEME")
  bettingSchemeName       String?                  @map("BETTING_SCHEME_NAME")
  bettingCutPercent       Float?                   @map("BETTING_CUT_PERCENT")
  belgianShow1            Float?                   @map("BELGIAN_SHOW_1")
  belgianShow2            Float?                   @map("BELGIAN_SHOW_2")
  belgianShow3            Float?                   @map("BELGIAN_SHOW_3")
  belgianShow4            Float?                   @map("BELGIAN_SHOW_4")
  belgianShow5            Float?                   @map("BELGIAN_SHOW_5")
  belgianShow6            Float?                   @map("BELGIAN_SHOW_6")
  belgianShow7            Float?                   @map("BELGIAN_SHOW_7")
  standardShow1           Float?                   @map("STANDARD_SHOW_1")
  standardShow2           Float?                   @map("STANDARD_SHOW_2")
  standardShow3           Float?                   @map("STANDARD_SHOW_3")
  standardShow4           Float?                   @map("STANDARD_SHOW_4")
  standardShow5           Float?                   @map("STANDARD_SHOW_5")
  standardShow6           Float?                   @map("STANDARD_SHOW_6")
  wta1                    Float?                   @map("WTA_1")
  wta2                    Float?                   @map("WTA_2")
  wta3                    Float?                   @map("WTA_3")
  wta4                    Float?                   @map("WTA_4")
  wta5                    Float?                   @map("WTA_5")
  creatorId               Int?                     @map("CREATOR_ID")
  creator                 OrganizerData?           @relation(fields: [creatorId], references: [id])
  bettingSchemeEvents     Events[]                 @relation("BETTINGSCHEMEEVENTS")
  standardShowPercentages StandardShowPercentage[]

  @@map("BettingScheme")
}

model Birds {
  idBird              Int                  @id @default(autoincrement()) @map("ID_BIRD")
  band                String?              @map("BAND")
  band1               String?              @map("BAND_1")
  band2               String?              @map("BAND_2")
  band3               String?              @map("BAND_3")
  band4               String?              @map("BAND_4")
  birdName            String?              @map("BIRD_NAME")
  rfId                String?              @map("RF_ID")
  color               String?              @map("COLOR")
  sex                 Int?                 @map("SEX")
  isActive            Int?                 @map("IS_ACTIVE")
  isRaceVerified      Int?                 @map("IS_RACE_VERIFIED")
  isLost              Int?                 @map("IS_LOST")
  lostDate            DateTime?            @map("LOST_DATE")
  lostIdRace          Int?                 @map("LOST_ID_RACE")
  note                String?              @map("NOTE")
  idPicture           Int?                 @map("ID_PICTURE")
  playAttentionSound  Int?                 @map("PLAY_ATTENTION_SOUND")
  breederId           Int?
  breeder             Breeders?            @relation(fields: [breederId], references: [idBreeder])
  lostRace            Race?                @relation("LOSTRACE", fields: [lostIdRace], references: [idRace])
  picture             Picture?             @relation("BIRDPICTURE", fields: [idPicture], references: [idPicture])
  eventInventoryItems EventInventoryItem[]
  lostHistories       LostHistory[]
  racePhantomBirds    RacePhantomBird[]

  @@map("Birds")
}

model BirdsView {
  idBird             Int       @id @default(autoincrement()) @map("ID_BIRD")
  band               String?   @map("BAND")
  band1              String?   @map("BAND_1")
  band2              String?   @map("BAND_2")
  band3              String?   @map("BAND_3")
  band4              String?   @map("BAND_4")
  birdName           String?   @map("BIRD_NAME")
  rfId               String?   @map("RF_ID")
  color              String?   @map("COLOR")
  sex                Int?      @map("SEX")
  isActive           Int?      @map("IS_ACTIVE")
  isRaceVerified     Int?      @map("IS_RACE_VERIFIED")
  isLost             Int?      @map("IS_LOST")
  lostDate           DateTime? @map("LOST_DATE")
  lostIdRace         Int?      @map("LOST_ID_RACE")
  note               String?   @map("NOTE")
  idPicture          Int?      @map("ID_PICTURE")
  playAttentionSound Int?      @map("PLAY_ATTENTION_SOUND")
  lostRace           Race?     @relation("VIEWLOSTRACE", fields: [lostIdRace], references: [idRace])
  picture            Picture?  @relation("VIEWBIRDPICTURE", fields: [idPicture], references: [idPicture])

  @@map("BirdsView")
}

model Breeders {
  idBreeder            Int              @id @default(autoincrement()) @map("ID_BREEDER")
  number               Int?             @map("NUMBER")
  firstName            String?          @map("FIRST_NAME")
  lastName             String?          @map("LAST_NAME")
  country              String?          @map("COUNTRY")
  isDefaultAddress1    Int?             @map("IS_DEFAULT_ADDRESS_1")
  address1             String?          @map("ADDRESS_1")
  city1                String?          @map("CITY_1")
  state1               String?          @map("STATE_1")
  zip1                 String?          @map("ZIP_1")
  address2             String?          @map("ADDRESS_2")
  city2                String?          @map("CITY_2")
  state2               String?          @map("STATE_2")
  zip2                 String?          @map("ZIP_2")
  phone                String?          @map("PHONE")
  cell                 String?          @map("CELL")
  fax                  String?          @map("FAX")
  email                String?          @map("EMAIL")
  email2               String?          @map("EMAIL_2")
  webAddress           String?          @map("WEB_ADDRESS")
  socialSecurityNumber String?          @map("SOCIAL_SECURITY_NUMBER")
  status               Int?             @map("STATUS")
  statusDate           DateTime?        @map("STATUS_DATE")
  note                 String?          @map("NOTE")
  loginName            String?          @map("LOGIN_NAME")
  loginPassword        String?          @map("LOGIN_PASSWORD")
  idPicture            Int?             @map("ID_PICTURE")
  sms                  String?          @map("SMS")
  taxNumber            String?          @map("TAX_NUMBER")
  defNameAgn           String?          @map("DEF_NAME_AGN")
  defNameAs            String?          @map("DEF_NAME_AS")
  picture              Picture?         @relation(fields: [idPicture], references: [idPicture])
  eventInventories     EventInventory[]
  partners             Partners[]
  birds                Birds[]
  payments             Payments[]

  @@map("Breeders")
}

model DatasyncSys {
  id       Int    @id @default(autoincrement()) @map("ID")
  dsExtVer Float? @map("DSEXT_VER")

  @@map("DatasyncSys")
}

model DatasyncWeb {
  id               Int     @id @default(autoincrement()) @map("ID")
  processed        Int?    @map("PROCESSED")
  dbTable          String? @map("DBTABLE")
  dbOperation      String? @map("DBOPERATION")
  dbTableIdColName String? @map("DBTABLEIDCOLNAME")
  dbTableRowId     Int?    @map("DBTABLEROWID")
  dbTableRow       Bytes?  @map("DBTABLEROW")

  @@map("DatasyncWeb")
}

model Events {
  idEvent                 Int               @id @default(autoincrement()) @map("ID_EVENT")
  eventName               String?           @map("EVENT_NAME")
  eventShortName          String?           @map("EVENT_SHORT_NAME")
  eventDate               DateTime?         @map("EVENT_DATE")
  idFeeScheme             Int?              @map("ID_FEE_SCHEME")
  feeScheme               FeeScheme?        @relation(fields: [idFeeScheme], references: [idFeeScheme])
  idFinalPrizeScheme      Int?              @map("ID_FINAL_PRIZE_SCHEME")
  finalPrizeScheme        PrizeScheme?      @relation(fields: [idFinalPrizeScheme], references: [idPrizeScheme], name: "FINALPRIZEEVENTS")
  idHotSpot1PrizeScheme   Int?              @map("ID_HOT_SPOT1_PRIZE_SCHEME")
  hotSpot1PrizeScheme     PrizeScheme?      @relation(fields: [idHotSpot1PrizeScheme], references: [idPrizeScheme], name: "HOTSPOT1PRIZEEVENTS")
  idHotSpot2PrizeScheme   Int?              @map("ID_HOT_SPOT2_PRIZE_SCHEME")
  hotSpot2PrizeScheme     PrizeScheme?      @relation(fields: [idHotSpot2PrizeScheme], references: [idPrizeScheme], name: "HOTSPOT2PRIZEEVENTS")
  idHotSpot3PrizeScheme   Int?              @map("ID_HOT_SPOT3_PRIZE_SCHEME")
  hotSpot3PrizeScheme     PrizeScheme?      @relation(fields: [idHotSpot3PrizeScheme], references: [idPrizeScheme], name: "HOTSPOT3PRIZEEVENTS")
  idHotSpotAvgPrizeScheme Int?              @map("ID_HOT_SPOT_AVG_PRIZE_SCHEME")
  hotSpotAvgPrizeScheme   PrizeScheme?      @relation(fields: [idHotSpotAvgPrizeScheme], references: [idPrizeScheme], name: "HOTSPOTAVGPRIZEEVENTS")
  idBettingScheme         Int?              @map("ID_BETTING_SCHEME")
  bettingScheme           BettingScheme?    @relation(fields: [idBettingScheme], references: [idBettingScheme], name: "BETTINGSCHEMEEVENTS")
  isOpen                  Int?              @map("IS_OPEN")
  eventType               Int?              @map("EVENT_TYPE")
  creatorId               Int?              @map("CREATOR_ID")
  creator                 OrganizerData?    @relation(fields: [creatorId], references: [id])
  avgWinnerPrizes         AvgWinnerPrizes[] @relation("AVGEVENTPRIZES")
  eventInventories        EventInventory[]
  eventRaceNumbers        EventRaceNumber[]
  prizeValues             PrizeValue[]
  races                   Race[]
  racePhantomBirds        RacePhantomBird[]

  @@map("Events")
}

model EventInventory {
  idEventInventory    Int                  @id @default(autoincrement()) @map("ID_EVENT_INVENTORY")
  idEvent             Int?                 @map("ID_EVENT")
  idBreeder           Int?                 @map("ID_BREEDER")
  signInDate          DateTime?            @map("SIGN_IN_DATE")
  waitingDate         DateTime?            @map("WAITING_DATE")
  isWaiting           Int?                 @map("IS_WAITING")
  reservedBirds       Int?                 @map("RESERVED_BIRDS")
  loft                String?              @map("LOFT")
  note                String?              @map("NOTE")
  event               Events?              @relation(fields: [idEvent], references: [idEvent])
  breeder             Breeders?            @relation(fields: [idBreeder], references: [idBreeder])
  eventInventoryItems EventInventoryItem[]
  payments            Payments[]
  partners            Partners[]

  @@map("EventInventory")
}

model EventInventoryItem {
  idEventInventoryItem Int                  @id @default(autoincrement()) @map("ID_EVENT_INVENTORY_ITEM")
  idEventInventory     Int?                 @map("ID_EVENT_INVENTORY")
  idBird               Int?                 @map("ID_BIRD")
  birdNo               Int?                 @map("BIRD_NO")
  arrivalTime          DateTime?            @map("ARRIVAL_DATE")
  departureDate        DateTime?            @map("DEPARTURE_DATE")
  perchFeeValue        Float?               @map("PERCH_FEE_VALUE")
  entryFeeValue        Float?               @map("ENTRY_FEE_VALUE")
  entryFeePaid         Int?                 @map("ENTRY_FEE_PAID")
  entryRefund          Float?               @map("ENTRY_REFUND")
  betsRefund           Float?               @map("BETS_REFUND")
  hotSpotFeeValue      Float?               @map("HOT_SPOT_FEE_VALUE")
  hotSpotRefund        Float?               @map("HOT_SPOT_REFUND")
  idReplacedItem       Int?                 @map("ID_REPLACED_ITEM")
  isBackup             Int?                 @map("IS_BACKUP")
  belgianShowBet1      Float?               @map("BELGIAN_SHOW_BET_1")
  belgianShowBet2      Float?               @map("BELGIAN_SHOW_BET_2")
  belgianShowBet3      Float?               @map("BELGIAN_SHOW_BET_3")
  belgianShowBet4      Float?               @map("BELGIAN_SHOW_BET_4")
  belgianShowBet5      Float?               @map("BELGIAN_SHOW_BET_5")
  belgianShowBet6      Float?               @map("BELGIAN_SHOW_BET_6")
  belgianShowBet7      Float?               @map("BELGIAN_SHOW_BET_7")
  standardShowBet1     Float?               @map("STANDARD_SHOW_BET_1")
  standardShowBet2     Float?               @map("STANDARD_SHOW_BET_2")
  standardShowBet3     Float?               @map("STANDARD_SHOW_BET_3")
  standardShowBet4     Float?               @map("STANDARD_SHOW_BET_4")
  standardShowBet5     Float?               @map("STANDARD_SHOW_BET_5")
  standardShowBet6     Float?               @map("STANDARD_SHOW_BET_6")
  wtaBet1              Float?               @map("WTA_BET_1")
  wtaBet2              Float?               @map("WTA_BET_2")
  wtaBet3              Float?               @map("WTA_BET_3")
  wtaBet4              Float?               @map("WTA_BET_4")
  wtaBet5              Float?               @map("WTA_BET_5")
  isBetActive          Float?               @map("IS_BET_ACTIVE")
  transferDue          Int?                 @map("TRANSFER_DUE")
  eventInventory       EventInventory?      @relation(fields: [idEventInventory], references: [idEventInventory])
  bird                 Birds?               @relation(fields: [idBird], references: [idBird])
  replacedItem         EventInventoryItem?  @relation("REPLACEDITEM", fields: [idReplacedItem], references: [idEventInventoryItem])
  replacementItems     EventInventoryItem[] @relation("REPLACEDITEM")
  raceIgnoreBirds      RaceIgnoreBird[]
  raceItems            RaceItem[]
  tmpBets              TmpBets[]

  @@map("EventInventoryItem")
}

model EventRaceNumber {
  idNumberGroup   Int              @map("ID_NUMBER_GROUP")
  idEvent         Int              @map("ID_EVENT")
  numberRangeFrom Int?             @map("NUMBER_RANGE_FROM")
  numberRangeTo   Int?             @map("NUMBER_RANGE_TO")
  numberGroup     RaceNumberGroup? @relation(fields: [idNumberGroup], references: [idRaceNumberGroup])
  event           Events?          @relation(fields: [idEvent], references: [idEvent])

  @@unique([idNumberGroup, idEvent])
  @@map("EventRaceNumber")
}

model FeeScheme {
  idFeeScheme        Int            @id @default(autoincrement()) @map("ID_FEE_SCHEME")
  feeSchemeName      String?        @map("FEE_SCHEME_NAME")
  entryFee           Int?           @map("ENTRY_FEE")
  isRefundable       Int?           @map("IS_REFUNDABLE")
  minEntryFees       Int?           @map("MIN_ENTRY_FEES")
  maxBirdCount       Int?           @map("MAX_BIRD_COUNT")
  maxBackupBirdCount Int?           @map("MAX_BACKUP_BIRD_COUNT")
  isFloatingBackup   Int?           @map("IS_FLOATING_BACKUP")
  feesCutPercent     Int?           @map("FEES_CUT_PERCENT")
  hotSpot1Fee        Int?           @map("HOT_SPOT1_FEE")
  hotSpot2Fee        Int?           @map("HOT_SPOT2_FEE")
  hotSpot3Fee        Int?           @map("HOT_SPOT3_FEE")
  hotSpotFinalFee    Int?           @map("HOT_SPOT_FINAL_FEE")
  creatorId          Int?           @map("CREATOR_ID")
  creator            OrganizerData? @relation(fields: [creatorId], references: [id])
  events             Events[]
  perchFeeItems      PerchFeeItem[]

  @@map("FeeScheme")
}

model IbeLogBlobFields {
  logTablesId  Int     @id @map("LOG_TABLES_ID")
  fieldName    String? @map("FIELD_NAME")
  oldCharValue String? @map("OLD_CHAR_VALUE")
  newCharValue String? @map("NEW_CHAR_VALUE")
  oldBlobValue Bytes?  @map("OLD_BLOB_VALUE")
  newBlobValue Bytes?  @map("NEW_BLOB_VALUE")

  @@map("IbeLogBlobFields")
}

model IbeLogFields {
  logTablesId Int     @id @map("LOG_TABLES_ID")
  fieldName   String? @map("FIELD_NAME")
  oldValue    String? @map("OLD_VALUE")
  newValue    String? @map("NEW_VALUE")

  @@map("IbeLogFields")
}

model IbeLogKeys {
  logTablesId Int     @id @map("LOG_TABLES_ID")
  keyField    String? @map("KEY_FIELD")
  keyValue    String? @map("KEY_VALUE")

  @@map("IbeLogKeys")
}

model IbeLogTables {
  id        Int       @id @map("ID")
  tableName String?   @map("TABLE_NAME")
  operation String?   @map("OPERATION")
  dateTime  DateTime? @map("DATE_TIME")
  userName  String?   @map("USER_NAME")

  @@map("IbeLogTables")
}

model Log {
  idLog          Int                @id @default(autoincrement()) @map("ID_LOG")
  logLevel       String?            @map("LOG_LEVEL")
  category       String?            @map("CATEGORY")
  content        String?            @map("CONTENT")
  dtLogged       DateTime?          @map("DT_LOGGED")
  idUser         Int?               @map("ID_USER")
  machine        String?            @map("MACHINE")
  user           Users?             @relation(fields: [idUser], references: [idUser])
  userLoggedins  LogUserLoggedin[]
  userLoggedouts LogUserLoggedout[]

  @@map("Log")
}

model LogUserLoggedin {
  idLog  Int   @map("ID_LOG")
  idUser Int   @map("ID_USER")
  log    Log   @relation(fields: [idLog], references: [idLog])
  user   Users @relation(fields: [idUser], references: [idUser])

  @@unique([idLog, idUser])
  @@map("LogUserLoggedin")
}

model LogUserLoggedout {
  idLog      Int @map("ID_LOG")
  idLoggedIn Int @map("ID_LOGGED_IN")
  log        Log @relation(fields: [idLog], references: [idLog])

  @@unique([idLog, idLoggedIn])
  @@map("LogUserLoggedout")
}

model LostHistory {
  idLostHistory Int       @id @default(autoincrement()) @map("ID_LOST_HISTORY")
  idBird        Int?      @map("ID_BIRD")
  lostDate      DateTime? @map("LOST_DATE")
  isLost        Int?      @map("IS_LOST")
  idRace        Int?      @map("ID_RACE")
  bird          Birds?    @relation(fields: [idBird], references: [idBird])
  race          Race?     @relation(fields: [idRace], references: [idRace])

  @@map("LostHistory")
}

model OrganizerData {
  id             Int             @id @default(autoincrement()) @map("ID")
  name           String?         @map("NAME")
  address        String?         @map("ADDRESS")
  city           String?         @map("CITY")
  state          String?         @map("STATE")
  zip            String?         @map("ZIP")
  email          String          @map("EMAIL")
  web            String?         @map("WEB")
  phone          String?         @map("PHONE")
  cell           String?         @map("CELL")
  fax            String?         @map("FAX")
  note           String?         @map("NOTE")
  password       String?         @map("PASSWORD")
  events         Events[]
  prizeSchemes   PrizeScheme[]
  feeSchemes     FeeScheme[]
  bettingSchemes BettingScheme[]

  @@map("OrganizerData")
}

model Partners {
  idEventInventory Int            @map("ID_EVENT_INVENTORY")
  idBreeder        Int            @map("ID_BREEDER")
  eventInventory   EventInventory @relation(fields: [idEventInventory], references: [idEventInventory])
  breeder          Breeders       @relation(fields: [idBreeder], references: [idBreeder])

  @@unique([idEventInventory, idBreeder])
  @@map("Partners")
}

model Payments {
  idPayment        Int             @id @default(autoincrement()) @map("ID_PAYMENT")
  idEventInventory Int?            @map("ID_EVENT_INVENTORY")
  paymentType      Int?            @map("PAYMENT_TYPE")
  paymentDate      DateTime?       @map("PAYMENT_DATE")
  paymentValue     Float?          @map("PAYMENT_VALUE")
  paymentMethod    Int?            @map("PAYMENT_METHOD")
  paymentDesc      String?         @map("PAYMENT_DESC")
  paymentTimestamp DateTime?       @map("PAYMENT_TIMESTAMP")
  transactionId    String?         @unique @map("TRANSACTION_ID")
  idPicture        Int?            @map("ID_PICTURE")
  status           Int?            @map("STATUS")
  idBreeder        Int?            @map("ID_BREEDER")
  breeder          Breeders?       @relation(fields: [idBreeder], references: [idBreeder])
  eventInventory   EventInventory? @relation(fields: [idEventInventory], references: [idEventInventory])
  picture          Picture?        @relation(fields: [idPicture], references: [idPicture])

  @@map("Payments")
}

model PerchFeeItem {
  idPerchFeeItem Int        @id @default(autoincrement()) @map("ID_PERCH_FEE_ITEM")
  birdNo         Int?       @map("BIRD_NO")
  perchFee       Int?       @map("PERCH_FEE")
  idFeeScheme    Int?       @map("ID_FEE_SCHEME")
  feeScheme      FeeScheme? @relation(fields: [idFeeScheme], references: [idFeeScheme])

  @@map("PerchFeeItem")
}

model Picture {
  idPicture     Int         @id @default(autoincrement()) @map("ID_PICTURE")
  picture       Bytes?      @map("PICTURE")
  pictureType   String?     @map("PICTURE_TYPE")
  pictureLayout String?     @map("PICTURE_LAYOUT")
  birds         Birds[]     @relation("BIRDPICTURE")
  birdsView     BirdsView[] @relation("VIEWBIRDPICTURE")
  breeders      Breeders[]
  payments      Payments[]

  @@map("Picture")
}

model PrizeScheme {
  idPrizeScheme         Int               @id @default(autoincrement()) @map("ID_PRIZE_SCHEME")
  prizeName             String?           @map("PRIZE_NAME")
  hotSpot1PrizeEvents   Events[]          @relation("HOTSPOT1PRIZEEVENTS")
  hotSpot2PrizeEvents   Events[]          @relation("HOTSPOT2PRIZEEVENTS")
  hotSpot3PrizeEvents   Events[]          @relation("HOTSPOT3PRIZEEVENTS")
  hotSpotAvgPrizeEvents Events[]          @relation("HOTSPOTAVGPRIZEEVENTS")
  finalPrizeEvents      Events[]          @relation("FINALPRIZEEVENTS")
  prizeSchemeItems      PrizeSchemeItem[]

  creatorId Int?           @map("CREATOR_ID")
  creator   OrganizerData? @relation(fields: [creatorId], references: [id])

  @@map("PrizeScheme")
}

model PrizeSchemeItem {
  idPrizeSchemeItem Int          @id @default(autoincrement()) @map("ID_PRIZE_SCHEME_ITEM")
  fromPosition      Int?         @map("FROM_POSITION")
  toPosition        Int?         @map("TO_POSITION")
  prizeValue        Float?       @map("PRIZE_VALUE")
  idPrizeScheme     Int?         @map("ID_PRIZE_SCHEME")
  prizeScheme       PrizeScheme? @relation(fields: [idPrizeScheme], references: [idPrizeScheme])
  prizeValues       PrizeValue[]

  @@map("PrizeSchemeItem")
}

model PrizeValue {
  idPrizeValue      Int              @id @default(autoincrement()) @map("ID_PRIZE_VALUE")
  idEvent           Int?             @map("ID_EVENT")
  idRaceType        Int?             @map("ID_RACE_TYPE")
  idPrizeSchemeItem Int?             @map("ID_PRIZE_SCHEME_ITEM")
  prizeValue        Float?           @map("PRIZE_VALUE")
  event             Events?          @relation(fields: [idEvent], references: [idEvent])
  raceType          RaceType?        @relation(fields: [idRaceType], references: [idRaceType])
  prizeSchemeItem   PrizeSchemeItem? @relation(fields: [idPrizeSchemeItem], references: [idPrizeSchemeItem])

  @@map("PrizeValue")
}

model Race {
  idRace             Int               @id @default(autoincrement()) @map("ID_RACE")
  idRaceType         Int?              @map("ID_RACE_TYPE")
  raceNumber         Int?              @map("RACE_NUMBER")
  idEvent            Int?              @map("ID_EVENT")
  location           String?           @map("LOCATION")
  description        String?           @map("DESCRIPTION")
  distance           Int?              @map("DISTANCE")
  startTime          DateTime?         @map("START_TIME")
  endTime            DateTime?         @map("END_TIME")
  sunrise            DateTime?         @map("SUNRISE")
  sunset             DateTime?         @map("SUNSET")
  isClosed           Int?              @map("IS_CLOSED")
  weather            String?           @map("WEATHER")
  wind               String?           @map("WIND")
  temperature        String?           @map("TEMPERATURE")
  arrivalWeather     String?           @map("ARRIVAL_WEATHER")
  arrivalWind        String?           @map("ARRIVAL_WIND")
  arrivalTemperature String?           @map("ARRIVAL_TEMPERATURE")
  raceType           RaceType?         @relation(fields: [idRaceType], references: [idRaceType])
  event              Events?           @relation(fields: [idEvent], references: [idEvent])
  baskets            Basket[]
  lostBirds          Birds[]           @relation("LOSTRACE")
  viewLostBirds      BirdsView[]       @relation("VIEWLOSTRACE")
  lostHistories      LostHistory[]
  raceIgnoreBirds    RaceIgnoreBird[]
  raceItems          RaceItem[]
  raceItemScans      RaceItemScan[]
  racePhantomBirds   RacePhantomBird[]

  @@map("Race")
}

model RaceIgnoreBird {
  idIgnoreBird         Int                 @id @default(autoincrement()) @map("ID_IGNORE_BIRD")
  idRace               Int?                @map("ID_RACE")
  idEventInventoryItem Int?                @map("ID_EVENT_INVENTORY_ITEM")
  note                 String?             @map("NOTE")
  race                 Race?               @relation(fields: [idRace], references: [idRace])
  eventInventoryItem   EventInventoryItem? @relation(fields: [idEventInventoryItem], references: [idEventInventoryItem])

  @@map("RaceIgnoreBird")
}

model RaceItem {
  idRaceItem      Int                 @id @default(autoincrement()) @map("ID_RACE_ITEM")
  idRace          Int?                @map("ID_RACE")
  idInventoryItem Int?                @map("ID_INVENTORY_ITEM")
  idDistBasket    Int?                @map("ID_DIST_BASKET")
  idRaceBasket    Int?                @map("ID_RACE_BASKET")
  isDistBasketted Int?                @map("IS_DIST_BASKETED")
  isLost          Int?                @map("IS_LOST")
  lostIdRace      Int?                @map("LOST_ID_RACE")
  raceBasketTime  DateTime?           @map("RACE_BASKET_TIME")
  race            Race?               @relation(fields: [idRace], references: [idRace])
  inventoryItem   EventInventoryItem? @relation(fields: [idInventoryItem], references: [idEventInventoryItem])
  raceItemResult  RaceItemResult?
  raceItemScans   RaceItemScan[]
  distBasket      Basket?             @relation("DistBasketItems", fields: [idDistBasket], references: [idBasket])
  raceBasket      Basket?             @relation("RaceBasketItems", fields: [idRaceBasket], references: [idBasket])

  @@map("RaceItem")
}

model RaceItemResult {
  idRaceItem          Int       @id @map("ID_RACE_ITEM") // This is a 1-to-1 relation, so it shouldn't auto-increment
  birdPosition        Int?      @map("BIRD_POSITION")
  birdPositionHotSpot Int?      @map("BIRD_POSITION_HOT_SPOT")
  arrivalTime         DateTime? @map("ARRIVAL_TIME")
  birdDrop            Int?      @map("BIRD_DROP")
  prizeValue          Float?    @map("PRIZE_VALUE")
  raceItem            RaceItem  @relation(fields: [idRaceItem], references: [idRaceItem])

  @@map("RaceItemResult")
}

model RaceItemScan {
  idRace     Int      @map("ID_RACE")
  idRaceItem Int      @map("ID_RACE_ITEM")
  scanPos    Int?     @map("SCAN_POS")
  race       Race     @relation(fields: [idRace], references: [idRace])
  raceItem   RaceItem @relation(fields: [idRaceItem], references: [idRaceItem])

  @@unique([idRace, idRaceItem])
  @@map("RaceItemScan")
}

model RaceNumberGroup {
  idRaceNumberGroup Int               @id @default(autoincrement()) @map("ID_RACE_NUMBER_GROUP")
  eventRaceNumbers  EventRaceNumber[]
  raceTypes         RaceType[]

  @@map("RaceNumberGroup")
}

model RacePhantomBird {
  idRacePhantomBird Int       @id @default(autoincrement()) @map("ID_RACE_PHANTOM_BIRD")
  idRace            Int?      @map("ID_RACE")
  idBird            Int?      @map("ID_BIRD")
